{
  "manifest_version": 3,
  "name": "簡単サムネイルダウンロード - Easy Thumbnail Downloader",
  "version": "0.1.2",
  "description": "Web ページ上に表示されているサムネイル画像を簡単にダウンロードできる拡張機能",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "type": "module"
  }
}
